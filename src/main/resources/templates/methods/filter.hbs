{{#partial "contenido"}} 
 <title>Filtro Metodologias</title>
 <p class="title">Metodologias filtradas</p>

 {{#if}}

  <table class="table table-dark">
   <tr>
    <th class="ppal">Nombre</th>
    <th class="ppal">Ver</th>
    <th class="ppal">Borrar</th>
   </tr>
   
   {{#each}}
    <tr>
     <td>{{name}}</td>
     <td><a class="fa fa-eye" href="{{id}}"></a></td>
     <td><a class="fa fa-trash-o clickable" onclick="eraseMethod({{id}})"></a></td>    
    </tr>
   {{/each}}
  </table>
  
 {{else}}
  <div class="alert alert-danger" role="alert">
     No hay metodologias para mostrar
  </div>
 {{/if}}
{{/partial}}

{{>layout.hbs}}